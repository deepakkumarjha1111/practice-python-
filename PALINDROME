t=int(input())
for p in range(t): 
    s=input()
    i=0;j=int(len(s)-1)
    flag=0
    if((j+1)%2==0):
        e=0
    else:
        e=1
        
        
    while(i<=j and flag!=1):
        if(s[i]==s[j]):
            i+=1
            j-=1
        else:
            flag=1
    if(flag==0):
        if e==0:
            print("YES EVEN")
        else:
            print("YES ODD")
    else:
       print("NO")
